    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "ava": "^1.0.1",
    "eslint-config-steelbrain": "^7.0.0-beta1",
    "flow-bin": "^0.89.0",
    "sb-babel-cli": "^1.0.2"
